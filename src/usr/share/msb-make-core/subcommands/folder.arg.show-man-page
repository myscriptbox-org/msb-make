source "$UTIL_FOLDER/args.fix-whitespace"
folder="$1"
source "$UTIL_FOLDER/folder.assert-exists"
folder_assert_exists "$folder"
$THIS_SCRIPT folder "$folder" -generate-man-page | man -l -

